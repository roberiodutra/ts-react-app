{
  "name": "djtiq-tsreact-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{tsx,js,jsx}\" --quiet",
    "format": "prettier --write \"src/**/*.{tsx,js,jsx}\"",
    "sass": "sass --watch src/styles/sass/styles.sass:src/styles/css/styles.css",
    "test": "jest --watchAll -i --forceExit --verbose --config=./jest.config.ts",
    "test:coverage": "react-scripts test --coverage --watchAll=false"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.9",
    "axios": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.37.0",
    "react-router-dom": "^6.4.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@swc/core": "^1.3.11",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "msw": "^0.47.4",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
